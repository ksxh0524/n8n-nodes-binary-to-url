{
  "name": "n8n-nodes-binary-to-url",
  "version": "0.1.1",
  "description": "n8n community node for creating temporary URLs for binary files within workflow execution",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "binary",
    "file",
    "temporary",
    "url",
    "memory",
    "workflow",
    "proxy",
    "webhook"
  ],
  "license": "MIT",
  "homepage": "",
  "author": {
    "name": "Binary Bridge Team"
  },
  "repository": {
    "type": "git",
    "url": "https://cnb.cool/ksxh-wwrs/n8n-nodes-binary-to-url"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "n8n-node build",
    "build:watch": "tsc --watch",
    "dev": "n8n-node dev",
    "format": "prettier --write \"**/*.{js,ts,json,md}\"",
    "lint": "n8n-node lint",
    "lint:fix": "n8n-node lint --fix",
    "release": "n8n-node release",
    "prepublishOnly": "n8n-node prerelease"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "strict": true,
    "nodes": [
      "dist/nodes/BinaryToUrl/BinaryToUrl.node.js"
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "@n8n/node-cli": "*",
    "eslint": "9.32.0",
    "eslint-plugin-n8n-nodes-base": "^1.11.0",
    "prettier": "3.6.2",
    "typescript": "5.9.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  }
}
